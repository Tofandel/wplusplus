{
  "name": "tofandel/wplusplus-core",
  "version": "1.9.2",
  "authors": [
    {
      "name": "Tofandel"
    }
  ],
  "type": "wordpress-plugin",
  "description": "W++ is a Wordpress Plugin acting as a core for other plugins",
  "license": "Apache 2.0",
  "keywords": [
    "mu-plugin",
    "wordpress",
    "options",
    "framework",
    "core",
    "boilerplate",
    "theme"
  ],
  "support": {
    "issues": "https://github.com/tofandel/wplusplus-core/issues"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "~1.0",
    "tgmpa/tgm-plugin-activation": ">=2.6",
    "yahnis-elsts/plugin-update-checker": "*"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Tofandel\\Core\\": "src/"
    }
  }
}